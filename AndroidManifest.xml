<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gamesgamestart"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="3"
        android:targetSdkVersion="9" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.games.gamestart.Start"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.games.gamestart.LifeCycleTest"
            android:label="@string/title_activity_life_cycle_test" >
        </activity>
        <activity
            android:name="com.games.gamestart.SingleTouchTest"
            android:label="@string/title_activity_single_touch_test" >
        </activity>
        <activity
            android:name="com.games.gamestart.MultiTouchTest"
            android:label="@string/title_activity_multi_touch_test" >
        </activity>
    </application>

</manifest>
